   <define name="TEI-history">
      <element name="history">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(histoire) rassemble les éléments servant à donner un historique complet du manuscrit ou d'une partie du manuscrit. [10.8. History]</a:documentation>
         <choice>
      
            <oneOrMore>
               <ref name="TEI-model.pLike"/>
            </oneOrMore>
      
            <group>
        
               <optional>
                  <ref name="TEI-summary"/>
               </optional>
        
        
          
        
        
               <zeroOrMore>
                  <ref name="TEI-provenance"/>
               </zeroOrMore>
        
        
          
        
            </group>
         </choice>
         <ref name="TEI-att.global.attributes"/>
         <empty/>
      </element>
