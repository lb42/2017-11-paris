   <define name="TEI-att.datable.iso.attributes">
      <ref name="TEI-att.datable.iso.attribute.when-iso"/>
      <ref name="TEI-att.datable.iso.attribute.notBefore-iso"/>
      <ref name="TEI-att.datable.iso.attribute.notAfter-iso"/>
      <ref name="TEI-att.datable.iso.attribute.from-iso"/>
      <ref name="TEI-att.datable.iso.attribute.to-iso"/>
   </define>
   <define name="TEI-att.datable.iso.attribute.when-iso">
      <optional>
         <attribute name="when-iso">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">précise une date exacte pour l'évènement selon la forme normalisée ISO 8601, c'est-à-dire aaaa-mm-jj.</a:documentation>
            <choice>
               <data type="date"/>
               <data type="gYear"/>
               <data type="gMonth"/>
               <data type="gDay"/>
               <data type="gYearMonth"/>
               <data type="gMonthDay"/>
               <data type="time"/>
               <data type="dateTime"/>
               <data type="token">
                  <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="TEI-att.datable.iso.attribute.notBefore-iso">
      <optional>
         <attribute name="notBefore-iso">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">précise la première date possible pour l'évènement selon la forme normalisée, c'est-à-dire aaaa-mm-jj.</a:documentation>
            <choice>
               <data type="date"/>
               <data type="gYear"/>
               <data type="gMonth"/>
               <data type="gDay"/>
               <data type="gYearMonth"/>
               <data type="gMonthDay"/>
               <data type="time"/>
               <data type="dateTime"/>
               <data type="token">
                  <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="TEI-att.datable.iso.attribute.notAfter-iso">
      <optional>
         <attribute name="notAfter-iso">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">précise la dernière date possible pour l'évènement selon la forme normalisée, c'est-à-dire aaaa-mm-jj.</a:documentation>
            <choice>
               <data type="date"/>
               <data type="gYear"/>
               <data type="gMonth"/>
               <data type="gDay"/>
               <data type="gYearMonth"/>
               <data type="gMonthDay"/>
               <data type="time"/>
               <data type="dateTime"/>
               <data type="token">
                  <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="TEI-att.datable.iso.attribute.from-iso">
      <optional>
         <attribute name="from-iso">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indique le point de départ de la période sous une forme normalisée</a:documentation>
            <choice>
               <data type="date"/>
               <data type="gYear"/>
               <data type="gMonth"/>
               <data type="gDay"/>
               <data type="gYearMonth"/>
               <data type="gMonthDay"/>
               <data type="time"/>
               <data type="dateTime"/>
               <data type="token">
                  <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="TEI-att.datable.iso.attribute.to-iso">
      <optional>
         <attribute name="to-iso">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indique le point final de la période sous une forme normalisée</a:documentation>
            <choice>
               <data type="date"/>
               <data type="gYear"/>
               <data type="gMonth"/>
               <data type="gDay"/>
               <data type="gYearMonth"/>
               <data type="gMonthDay"/>
               <data type="time"/>
               <data type="dateTime"/>
               <data type="token">
                  <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="TEI-orgName">
      <element name="orgName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom d'organisation) contient le nom d'une organisation.L'élément orgName peut être utilisé dans le corps de la transcription, pour baliser un nom de régiment, notamment lorsque celui-ci fait l'objet d'une notice. L'élément orgName sert aussi dans les notices descriptives des régiments ; dans ce cas il aura un attribut type. [13.2.2. Organizational Names]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-lb"/>
               <ref name="TEI-pb"/>
               <ref name="TEI-choice"/>
               <ref name="TEI-gap"/>
               <ref name="TEI-unclear"/>
               <ref name="TEI-supplied"/>
               <ref name="TEI-damage"/>
               <ref name="TEI-add"/>
               <ref name="TEI-del"/>
               <ref name="TEI-subst"/>
               <ref name="TEI-app"/>
               <ref name="TEI-hi"/>
               <ref name="TEI-placeName"/>
               <ref name="TEI-orgName"/>
               <ref name="TEI-ref"/>
               <ref name="TEI-num"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-orgName-contentIfOrgNameInAuthorityRecord-constraint-rule-38">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:orgName[parent::tei:org or ancestor::tei:person or ancestor::tei:respStmt]">
               <sch:assert test="not(tei:lb) and not(tei:pb) and not(tei:choice) and not(tei:gap) and not(tei:unclear) and not(tei:supplied) and not(tei:damage) and not(tei:add) and not(tei:del) and not(tei:subst) and not(tei:app) and not(tei:ref)">L'élément orgName ne devrait contenir, optionnellement, que les élément hi,
                  orgName, num et placeName.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-orgName-attrTypeIfOrgNameInAuthorityRecord-constraint-rule-39">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:orgName[parent::tei:org]">
               <sch:assert test="@type">Dans une notice sur une unité militaire, il convient de spécifier à quelle
                  catégorie la forme du nom appartient, en utilisant l'attribut
                type</sch:assert>
            </sch:rule>
         </pattern>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">caractérise l'élément en utilisant n'importe quel système ou typologie de classification approprié.Utiliser cet attribut, lorsque le nom de l'unité militaire est donné dans la notice sur l'unité militaire, pour spécifier si la forme du nom est saisie en langage naturel ou si elle est saisie comme une entrée d'index complète</a:documentation>
               <choice>
                  <value>proseForm</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(forme en français (langage naturel)) </a:documentation>
                  <value>fullIndexEntryForm</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(forme complète d'entrée d'index) </a:documentation>
                  <value>abbreviatedForm</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(forme abrégée du nom de l'unité militaire) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="ref">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(référence) fournit un moyen explicite de localiser une définition complète de l'entité nommée au moyen d'un ou plusieurs URIs.L'attribut ref contient, le cas échéant, un renvoi vers la notice décrivant, au sein de la plate-forme de transcription, l'unité militaire dont le nom a été balisé.</a:documentation>
               <data type="anyURI"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="xml:id">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifiant) fournit un identifiant unique pour l'élément qui porte l'attributCet attribut sert, dans les métadonnées du fichier TEI de transcription, pour assigner un identifiant à chacun des organismes contributeurs.</a:documentation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="corresp">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(correspond) pointe vers des éléments qui ont une correspondance avec l'élément en question.Cet attribut sert, dans les métadonnées du fichier TEI de transcription, pour établir une correspondance entre le nom d'un organisme contributeur et une autre occurrence du même nom, située plus haut dans le fichier et dotée d'un identifiant.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="anyURI"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-persName">
      <element name="persName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom de personne) contient un nom propre ou une expression nominale se référant à une personne, pouvant inclure tout ou partie de ses prénoms, noms de famille, titres honorifiques, noms ajoutés, etc.L'élément persNamepeut être utilisé dans le corps de la transcription pour baliser un nom de personne, notamment lorsque la personne fait l'objet d'une notice. On l'utilise notamment dans la ou les mentions de signature du testament. Dans ce cas, il est recommandé de distinguer, dans la chaîne de caractères ainsi balisée, le nom de famille de la personne (afin que celui-ci, ultérieurement, puisse être mis en forme de manière spécifique dans l'édition de restitution), à l'aide de l'élément surname. L'élément persName sert aussi pour baliser les métadonnées du testament et dans les notices sur les testateurs ; dans ce dernier cas, on devra trouver dans cet élément les sous-éléments distinguant nom de famille et prénoms. [13.2.1. Personal Names]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-lb"/>
               <ref name="TEI-pb"/>
               <ref name="TEI-choice"/>
               <ref name="TEI-gap"/>
               <ref name="TEI-unclear"/>
               <ref name="TEI-supplied"/>
               <ref name="TEI-damage"/>
               <ref name="TEI-add"/>
               <ref name="TEI-del"/>
               <ref name="TEI-subst"/>
               <ref name="TEI-app"/>
               <ref name="TEI-hi"/>
               <ref name="TEI-surname"/>
               <ref name="TEI-forename"/>
               <ref name="TEI-affiliation"/>
               <ref name="TEI-ref"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-persName-contentIfPersNameInSigned-constraint-rule-40">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:persName[ancestor::tei:signed]">
               <sch:assert test="tei:surname">Penser à baliser le nom de famille de la
                personne</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-persName-contentIfPersNameInAuthorityRecord-constraint-rule-41">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:persName[parent::tei:person and @type='fullIndexEntryForm']">
               <sch:assert test="tei:surname and tei:forename and not(tei:affiliation) and not(tei:lb) and not(tei:pb) and not(tei:choice) and not(tei:gap) and not(tei:unclear) and not(tei:supplied) and not(tei:damage) and not(tei:add) and not(tei:del) and not(tei:subst) and not(tei:app) and not(tei:ref) and not(tei:hi)">L'élément persName devrait contenir au moins un élément surname et un élément
                  forename, et ne peut pas contenir d'autres
              éléments</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-persName-contentIfPersNameInRespStmt-constraint-rule-42">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:persName[ancestor::tei:respStmt and not(ancestor::tei:bibl)]">
               <sch:assert test="@xml:id or @corresp">Dans une mention de responsabilité,
                  l'élément persName devrait avoir un identifiant (attribut xml:id) ou pointer vers
                  un autre élément persName via l'attribut
              @corresp</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-persName-attrTypeIfPersNameInAuthorityRecord-constraint-rule-43">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:persName[parent::tei:person]">
               <sch:assert test="@type">Dans une notice sur une personne, il convient de spécifier à quelle
                  catégorie la forme du nom appartient, en utilisant l'attribut
                type</sch:assert>
            </sch:rule>
         </pattern>
         <optional>
            <attribute name="ref">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(référence) fournit un moyen explicite de localiser une définition complète de l'entité nommée au moyen d'un ou plusieurs URIs.L'attribut ref contient, le cas échéant, un renvoi vers la notice décrivant, au sein de la plate-forme de transcription, la personne dont le nom a été balisé.</a:documentation>
               <data type="anyURI"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="xml:id">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifiant) fournit un identifiant unique pour l'élément qui porte l'attributCet attribut sert, dans les métadonnées du fichier TEI de transcription, pour assigner un identifiant à chacun des contributeurs.</a:documentation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="corresp">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(correspond) pointe vers des éléments qui ont une correspondance avec l'élément en question.Cet attribut sert, dans les métadonnées du fichier TEI de transcription, pour établir une correspondance entre le nom d'un contributeur et une autre occurrence du même nom, située plus haut dans le fichier et dotée d'un identifiant.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="anyURI"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">caractérise l'élément en utilisant n'importe quel système ou typologie de classification approprié.Utiliser cet attribut, lorsque le nom de personne est donné dans une notice sur la personne, pour spécifier si la forme du nom est saisie en langage naturel ou si elle est saisie comme une entrée d'index</a:documentation>
               <choice>
                  <value>fullProseForm</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(forme complète en français (langage naturel)) </a:documentation>
                  <value>fullIndexEntryForm</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(forme complète d'entrée d'index) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-surname">
      <element name="surname">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom de famille) contient un nom de famille (hérité) par opposition à un nom donné, nom de baptême ou surnom. [13.2.1. Personal Names]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-lb"/>
               <ref name="TEI-pb"/>
               <ref name="TEI-choice"/>
               <ref name="TEI-gap"/>
               <ref name="TEI-unclear"/>
               <ref name="TEI-supplied"/>
               <ref name="TEI-damage"/>
               <ref name="TEI-add"/>
               <ref name="TEI-del"/>
               <ref name="TEI-subst"/>
               <ref name="TEI-ref"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-surname-surnameNotEmpty-constraint-rule-44">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:surname">
               <sch:assert test="normalize-space(.)!=''">Si
                  vous utilisez l'élément surname, il ne devrait pas être vide ; il faut lui donner
                  un contenu textuel.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-surname-contentIfSurnameInAuthorityRecord-constraint-rule-45">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:surname[ancestor::tei:person]">
               <sch:assert test="not(tei:*)">Dans la forme autorisée du nom de la personne, l'élément surname
                  ne devrait contenir que du texte</sch:assert>
            </sch:rule>
         </pattern>
      </element>
   </define>
   <define name="TEI-forename">
      <element name="forename">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(prénom) contient un prénom, qu'il soit donné ou un nom de baptême.L'élément forename est utilisé au sein de l'élément persName (nom de personne), pour baliser par ex. chacun des prénoms dans une signature dans le corps de la transcription. Il est surtout employé dans les notices descriptives des testateurs, au sein de l'élément persName qui sert à enregistrer la forme complète, normalisée et segmentée, du nom de la personne. [13.2.1. Personal Names]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-lb"/>
               <ref name="TEI-pb"/>
               <ref name="TEI-choice"/>
               <ref name="TEI-gap"/>
               <ref name="TEI-unclear"/>
               <ref name="TEI-supplied"/>
               <ref name="TEI-damage"/>
               <ref name="TEI-add"/>
               <ref name="TEI-del"/>
               <ref name="TEI-subst"/>
               <ref name="TEI-ref"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-forename-forenameNotEmpty-constraint-rule-46">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:forename">
               <sch:assert test="normalize-space(.)!=''">Si vous utilisez l'élément forename, il ne devrait pas être vide ; il faut lui
                  donner un contenu textuel.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-forename-contentIfForenameInAuthorityRecord-constraint-rule-47">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:forename[ancestor::tei:person]">
               <sch:assert test="not(tei:*)">Dans la forme autorisée du nom de la personne, l'élément
                  forename ne devrait contenir que du texte</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="TEI-att.global.attributes"/>
         <ref name="TEI-att.personal.attributes"/>
         <ref name="TEI-att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="TEI-placeName">
      <element name="placeName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom de lieu) contient un nom de lieu absolu ou relatif.L'élément placeNamepeut être utilisé dans le corps de la transcription pour baliser un nom de lieu, notamment lorsque le lieu fait l'objet d'une notice. Dans ce cas, l'attribut type permettra de dire de quel type de lieu il s'agit (lieu de rédaction du testament, lieu de naissance ou de décès du testateur, etc.). L'élément placeName sert aussi pour baliser les métadonnées du testament. Enfin, cet élément est aussi utilisé dans les notices sur les lieux, pour baliser soit le nom du lieu décrit, soit le nom d'une commune, d'une région ou autre circonscription administrative dans lequel le lieu décrit se situe. Si placeName sert à encoder le nom du lieu décrit, on trouvera dans cet élément les sous-éléments distinguant les composants du nom de lieu : nom de lieu habité, nom de lieu géographique. Toujours dans le cas de la notice sur un lieu, le nom de lieu pourra le cas échéant avoir un attribut when-iso pour spécifier les dates d'utilisation de ce nom de lieu. [13.2.3. Place Names]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-lb"/>
               <ref name="TEI-pb"/>
               <ref name="TEI-choice"/>
               <ref name="TEI-gap"/>
               <ref name="TEI-unclear"/>
               <ref name="TEI-supplied"/>
               <ref name="TEI-damage"/>
               <ref name="TEI-add"/>
               <ref name="TEI-del"/>
               <ref name="TEI-subst"/>
               <ref name="TEI-app"/>
               <ref name="TEI-hi"/>
               <ref name="TEI-geogName"/>
               <ref name="TEI-geogFeat"/>
               <ref name="TEI-settlement"/>
               <ref name="TEI-ref"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-placeName-contentIfPlaceNameInPlaceAuthorityRecord-constraint-rule-48">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:placeName[parent::tei:place]">
               <sch:assert test="tei:settlement or tei:geogName">Dans une notice sur un lieu, l'élément
                  placeName donnant le nom du lieu devrait contenir au moins un sous-élément
                  settlement ou geogName</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-placeName-contentIfPlaceNameInPersonAuthorityRecord-constraint-rule-49">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:placeName[ancestor::tei:person or ancestor::tei:place]">
               <sch:assert test="not(tei:lb) and not(tei:pb) and not(tei:choice) and not(tei:gap) and not(tei:unclear) and not(tei:supplied) and not(tei:damage) and not(tei:add) and not(tei:del) and not(tei:subst) and not(tei:app) and not(tei:ref)">Dans une notice sur une personne ou sur un lieu, l'élément placeName ne devrait
                  pas contenir d'éléments dédiés à la
              transcription</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-placeName-attrWhen-isoIfPlaceNameInAuthorityRecord-constraint-rule-50">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:placeName[(parent::tei:place or ancestor::tei:location) and count(parent::tei:*/tei:placeName)&gt;1]">
               <sch:assert test="@when-iso">Dans une notice sur un lieu, si l'élément placeName
                  est utilisé au moins deux fois, chaque élément placeName devrait avoir un attribut
                  when-iso pour spécifier pendant quelle période le nom de lieu a été
                  utilisé</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-placeName-attrTypeIfPlaceNameInTranscriptionOrWillMetadata-constraint-rule-51">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:placeName[ancestor::tei:summary or ancestor::tei:div[@type='will'] or ancestor::tei:div[@type='envelope']]">
               <sch:assert test="@type">Dans les métadonnées sur le testament ou dans le corps
                  de la transcription, l'élément placeName devrait avoir un attribut
                  type</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-placeName-contentIfPlaceNameInTranscription-constraint-rule-52">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:placeName[ancestor::tei:div[@type='will'] or ancestor::tei:div[@type='envelope']]">
               <sch:assert test="not(tei:geogName) and not(tei:geogFeat)">Dans le corps de la
                  transcription, l'élément placeName ne devrait pas avoir de sous-élément geogName
                  ou settlement</sch:assert>
            </sch:rule>
         </pattern>
         <optional>
            <attribute name="ref">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(référence) fournit un moyen explicite de localiser une définition complète de l'entité nommée au moyen d'un ou plusieurs URIs.L'attribut ref contient, le cas échéant, un renvoi vers la notice décrivant, au sein de la plate-forme de transcription, le lieu dont le nom a été balisé.</a:documentation>
               <data type="anyURI"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">caractérise l'élément en utilisant n'importe quel système ou typologie de classification approprié.Utiliser cet attribut, lorsque le nom de lieu est balisé dans le corps de la transcription, pour spécifier de quel type de date il s'agit, afin de permettre, par la suite, des recherches par type de lieu.</a:documentation>
               <choice>
                  <value>willPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu de rédaction du testament) </a:documentation>
                  <value>codicilPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu de rédaction du codicille) </a:documentation>
                  <value>willAuthorBirthPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu de naissance du testateur) </a:documentation>
                  <value>willAuthorDeathPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu de décès du testateur) </a:documentation>
                  <value>willAuthorResidence</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(domicile du testateur) </a:documentation>
                  <value>militaryEventPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu d'un événement militaire (bataille,
                    offensive...)) </a:documentation>
                  <value>otherPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(autre type de lieu) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="when-iso">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(date exacte au format ISO 8601) précise une date exacte pour l'évènement selon la forme normalisée ISO 8601, c'est-à-dire aaaa-mm-jj.Cet attribut contient le cas échéant la forme normalisée de la période pendant laquelle le nom de lieu a été utilisé (dans une notice sur un lieu)</a:documentation>
               <choice>
                  <data type="date"/>
                  <data type="gYear"/>
                  <data type="gMonth"/>
                  <data type="gDay"/>
                  <data type="gYearMonth"/>
                  <data type="gMonthDay"/>
                  <data type="time"/>
                  <data type="dateTime"/>
                  <data type="token">
                     <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
                  </data>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-country">
      <element name="country">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(pays) contient le nom d'une unité géo-politique, comme une nation, un pays, une colonie ou une communauté, plus grande ou administrativement supérieure à une région et plus petite qu'un bloc.dans les notices descriptives des lieux, cet élément est obligatoire au sein de l'élément location servant à localiser le lieu dans un contexte géopolitique, pour spécifier dans quel pays se trouve le lieu. [13.2.3. Place Names]</a:documentation>
         <text/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-country-countryNotEmpty-constraint-rule-53">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:country">
               <sch:assert test="normalize-space(.)!=''">Si
                  vous utilisez l'élément country, il ne devrait pas être vide ; il faut lui donner
                  un contenu textuel.</sch:assert>
            </sch:rule>
         </pattern>
      </element>
   </define>
   <define name="TEI-region">
      <element name="region">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(région) contient le nom d'une unité administrative comme un état, une province ou un comté, plus grande qu'un lieu de peuplement, mais plus petite qu'un pays.dans les notices descriptives des lieux, region est utilisé pour encoder les noms des départements et régions administratives français (afin de localiser le lieu s'il est en France), et le nom des provinces, régions ou états étrangers lorsque le lieu est localisé dans un pays étranger et que l'on connaît ces informations. Dans le cas d'un lieu situé en France, l'élément region a un attribut type. [13.2.3. Place Names]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-hi"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-region-regionNotEmpty-constraint-rule-54">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:region">
               <sch:assert test="normalize-space(.)!=''">Si
                  vous utilisez l'élément region, il ne devrait pas être vide ; il faut lui donner
                  un contenu textuel.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-region-attrTypeIfRegionInFranceInAuthorityRecord-constraint-rule-55">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:region[normalize-space(parent::tei:location/tei:country)='France']">
               <sch:assert test="@type">Dans une notice sur un lieu, si l'élément region est
                  utilisé pour localiser le lieu en France, il devrait avoir un attribut
                  type.</sch:assert>
            </sch:rule>
         </pattern>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">le type de région</a:documentation>
               <choice>
                  <value>FrenchRegion</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(région administrative française) </a:documentation>
                  <value>FrenchDepartement</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(département français) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-settlement">
      <element name="settlement">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu de peuplement) contient le nom d'un lieu de peuplement comme une cité, une ville ou un village, identifié comme une unité géo-politique ou administrative unique. [13.2.3. Place Names]</a:documentation>
         <choice>
            <text/>
            <oneOrMore>
               <ref name="TEI-placeName"/>
            </oneOrMore>
         </choice>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-settlement-settlementNotEmpty-constraint-rule-56">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:settlement[parent::tei:placeName/parent::tei:place]">
               <sch:assert test="normalize-space(.)!=''">Si vous utilisez l'élément settlement
                  pour baliser le nom du lieu habité décrit, il ne devrait pas être vide ; il faut
                  lui donner un contenu textuel.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-settlement-settlementInLocationContainsAtLeastPLaceName-constraint-rule-57">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:settlement[parent::tei:location]">
               <sch:assert test="tei:placeName">Si vous utilisez l'élément settlement pour baliser le nom de
                  la commune dans laquelle le lieu habité ou géographique se situe, cet élément
                  devrait contenir au moins un élément
              placeName.</sch:assert>
            </sch:rule>
         </pattern>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(la catégorie de lieu habité) </a:documentation>
            <choice>
               <value>city</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(ville) </a:documentation>
               <value>locality</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(localité) pour tout lieu-dit, hameau, peuplement qui n'a pas le statut administratif de municipalité</a:documentation>
               <value>other</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="TEI-district">
      <element name="district">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(district) contient le nom d'une subdivision quelconque d'une ville, comme une paroisse, une circonscription électorale ou toute autre unité administrative ou géographique.cet élément est utilisé dans les notices descriptives des testateurs, au sein de l'élément address (adresse), lorsque l'adresse du testateur inclut la mention d'un arrondissement urbain. [13.2.3. Place Names]</a:documentation>
         <group>
            <text/>
            <optional>
               <ref name="TEI-hi"/>
            </optional>
         </group>
      </element>
   </define>
   <define name="TEI-geogName">
      <element name="geogName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom de lieu géographique) un nom associé à une caractéristique géographique comme Windrush Valley ou le Mont Sinaï.utilisé dans les notices descriptives des lieux, au sein de l'élément placeName, pour baliser le nom d'un lieu géographique (bois, forêt, ...). Cet élément contient parfois l'élément geogFeat (catégorie de lieu géographique). Il devrait porter un attribut type. [13.2.3. Place Names]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-geogFeat"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-geogName-geogNameNotEmpty-constraint-rule-58">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:geogName">
               <sch:assert test="normalize-space(.)!=''">Si vous utilisez l'élément geogName, il ne devrait pas être vide ; il faut lui
                  donner un contenu textuel.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-geogName-ifGeogNameThenAttrType-constraint-rule-59">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:geogName[ancestor::tei:place]">
               <sch:assert test="@type">Vous devriez catégoriser le nom de lieu géographique à l'aide de
                  l'attribut type</sch:assert>
            </sch:rule>
         </pattern>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(la catégorie de lieu géographique) </a:documentation>
            <choice>
               <value>mountain</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(mont) </a:documentation>
               <value>forest</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(forêt, bois) </a:documentation>
               <value>other</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="TEI-geogFeat">
      <element name="geogFeat">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom de caractéristique géographique) contient un nom commun identifiant une caractéristique géographique contenue dans un nom de lieu, comme vallée, mont, etc. [13.2.3. Place Names]</a:documentation>
         <text/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-geogFeat-geogFeatNotEmpty-constraint-rule-60">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:geogFeat">
               <sch:assert test="normalize-space(.)!=''">Si vous utilisez l'élément geogFeat, il ne devrait pas être vide ; il faut lui
                  donner un contenu textuel.</sch:assert>
            </sch:rule>
         </pattern>
      </element>
   </define>
   <define name="TEI-affiliation">
      <element name="affiliation">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(affiliation) contient une description non formalisée portant sur l'affiliation présente ou passée d'une personne à une organisation, par exemple un employeur ou un sponsor.cet élément est utilisé dans les notices sur les testateurs pour consigner les informations recueillies sur leur grade et leur unité militaire d'affectation (dans l'élément orgName). S'il existe une notice sur l'unité militaire, l'élément orgName devrait logiquement avoir un attribut ref renvoyant à cette notice. [15.2.2. The Participant Description]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-hi"/>
               <ref name="TEI-orgName"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-affiliation-useOrgNameInAffiliation-constraint-rule-61">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:affiliation">
               <sch:assert test="tei:orgName">Si vous
                  connaissez l'unité militaire d'affectation du testateur, vous devriez utiliser
                  l'élément orgName pour baliser le nom de cette
              unité.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-affiliation-ifExcludeThenAttXmlId-constraint-rule-62">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:affiliation[@exclude]">
               <sch:assert test="@xml:id">Si
                  vous utilisez l'attribut exclude pour cette affectation, il convient d'assigner un
                  identifiant à l'affectation (et aussi, mais ce n'est pas rendu obligatoire) de
                  spécifier quelle(s) source(s) donnent
              l'information.</sch:assert>
            </sch:rule>
         </pattern>
         <optional>
            <attribute name="xml:id">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifiant) fournit un identifiant unique pour l'élément qui porte l'attributl'identifiant unique de l'affiliation. A utiliser uniquement si l'affiliation qui est spécifiée est exclusive d'une autre affiliation pour la même personne (pour mémoire, dans ce cas, les éléments affiliation concernés devraient aussi avoir les attributs excl et source</a:documentation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="source">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(source) Cet attribut est utilisé pour pointer vers une ou plusieurs sources (références bibliographiques, sources archivistiques en ligne...) qui permettent d'établir l'affiliation.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="anyURI"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute name="exclude">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(exclusif (de)) pointe vers des éléments qui sont une alternative exclusive à l'élément en question.Cet attribut donne les identifiants des éléments affiliation qui constituent une alternative exclusive à l'affiliation spécifiée. Peut éventuellement servir dans le cas où une ou plusieurs sources indiquent plusieurs affiliations différentes pour le Poilu au même moment. Si cet attribut est présent, les attributs xml:id et source devraient l'être aussi.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="anyURI"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-birth">
      <element name="birth">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(naissance) contient des informations sur la naissance d'une personne, comme la date et le lieu [15.2.2. The Participant Description]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-hi"/>
               <ref name="TEI-date"/>
               <ref name="TEI-placeName"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-birth-birthShouldHaveAtLeastDate-constraint-rule-63">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:birth">
               <sch:assert test="tei:date">Il devrait y avoir
                  au moins un élément date dans birth.</sch:assert>
            </sch:rule>
         </pattern>
      </element>
   </define>
   <define name="TEI-death">
      <element name="death">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(décès) contient des informations sur le décès d'une personne, comme la date et le lieu.si ces informations sont connues (ce qui devrait normalement être le cas), cet élément contient, au sein d'une phrase, la transcription de la mention de décès telle que trouvée sur la minute, la date précise ou approximative (dans l'élément date) et le lieu de décès (dans l'élément placeName) du testateur. S'il existe une notice sur le lieu de décès, l'élément placeName devrait logiquement avoir un attribut ref renvoyant à cette notice. [15.2.2. The Participant Description]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-hi"/>
               <ref name="TEI-date"/>
               <ref name="TEI-placeName"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-death-deathShouldHaveAtLeastDate-constraint-rule-64">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:death">
               <sch:assert test="tei:date">Il devrait y avoir
                  au moins un élément date dans death.</sch:assert>
            </sch:rule>
         </pattern>
      </element>
   </define>
   <define name="TEI-geo">
      <element name="geo">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(coordonnées géographiques) contient toute expression, dans un système de notation, d'un ensemble de coordonnées géographiques représentant un point, une ligne ou une zone sur la surface de la Terre.cet élément est utilisé dans les notices descriptives des lieux, pour donner la latitude et la longitude du lieu décrit, selon le Système géodésique mondial (WGS84). La latitude et la longitude sont séparées par une espace. On peut les trouver en consultant, par ex., la base Geonames. [13.3.4.1. Varieties of Location]</a:documentation>
         <text/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-geo-geoNotEmpty-constraint-rule-65">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:geo">
               <sch:assert test="normalize-space(.)!=''">Si
                  vous utilisez l'élément geo, il ne devrait pas être vide ; il faut lui donner un
                  contenu textuel (saisir la latitude du lieu, une espace, puis la
                  longitude.</sch:assert>
            </sch:rule>
         </pattern>
      </element>
   </define>
   <define name="TEI-listOrg">
      <element name="listOrg">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(liste d'organisations) contient une liste d'éléments, chacun d'eux fournissant des informations sur une organisation identifiable.contient l'ensemble des notices produites dans le cadre du projet pour décrire les unites militaires (régiments, bataillons...) dans lesquelles les testateurs sont affectés au moment où ils rédigent leur testament. (CHECK : autres cas ?) [13.2.2. Organizational Names]</a:documentation>
         <oneOrMore>
            <ref name="TEI-org"/>
         </oneOrMore>
      </element>
   </define>
   <define name="TEI-listPerson">
      <element name="listPerson">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(liste de personnes) contient une liste d'éléments, chacun d'entre eux apportant des informations sur une personne précise ou sur un groupe de personnes, par exemple les participants à une interaction linguistique, ou les personnes citées dans une source historique.contient l'ensemble des notices produites dans le cadre du projet pour décrire les testateurs. (CHECK : et autres personnes ? si oui, penser aux relations familiales  [13.3.2. The Person Element 15.2. Contextual Information 2.4. The Profile Description 15.3.2. Declarable Elements]</a:documentation>
         <oneOrMore>
            <ref name="TEI-person"/>
         </oneOrMore>
      </element>
   </define>
   <define name="TEI-listPlace">
      <element name="listPlace">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(liste de lieux) contient une liste de lieux, qui peut être suivie d'une liste de relations définies entre les lieux (autres que la relation d'inclusion).contient l'ensemble des notices produites dans le cadre du projet pour décrire les lieux de naissance, de résidence et de décès des testateurs. (CHECK : autres cas ?) [2.2.7. The Source Description 13.3.4. Places]</a:documentation>
         <oneOrMore>
            <ref name="TEI-place"/>
         </oneOrMore>
      </element>
   </define>
   <define name="TEI-location">
      <element name="location">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(localisation) définit l'emplacement d'un lieu par des coordonnées géographiques, en termes d'entités nommées dites géopolitiques, ou par une adresse.utilisé dans les notices descriptives des lieux, pour localiser le lieu décrit, soit en déclarant les subdivisions administratives et géopolitiques auxquelles il appartient (il est alors obligatoire de spécifier au moins le pays actuel), soit via ses coordonnées géographiques. Dans le cas d'une localisation administrative, l'élément location pourra le cas échéant avoir un attribut when-iso pour spécifier les dates pendant lesquelles cette localisation a été effective. [13.3.4. Places]</a:documentation>
         <choice>
            <group>
               <optional>
                  <ref name="TEI-settlement"/>
               </optional>
               <zeroOrMore>
                  <ref name="TEI-region"/>
               </zeroOrMore>
               <ref name="TEI-country"/>
            </group>
            <optional>
               <ref name="TEI-geo"/>
            </optional>
         </choice>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-location-locationIfGeopoliticalHasAtLeastCountry-constraint-rule-66">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:location[not(tei:geo)]">
               <sch:assert test="tei:country[normalize-space(.)!='']">Si vous localisez un lieu par son
                  appartenance à des entités administratives ou géoolitiques, vous devriez au moins
                  spécifier dans quel pays il se trouve
              aujourd'hui.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-location-attrWhen-isoIfLocationInAuthorityRecord-constraint-rule-67">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:location[parent::tei:place and count(parent::tei:place/tei:location[tei:country])&gt;1 and not(tei:geo)]">
               <sch:assert test="@when-iso">Dans une notice sur un lieu, si l'élément location
                  est utilisé au moins deux fois pour spécifier une localisation géopolitique,
                  chaque élément location devrait avoir un attribut when-iso pour spécifier pendant
                  quelle période cette localisation a été
              effective</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-location-attrTypeIfGeographicalLocationInAuthorityRecord-constraint-rule-68">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:location[parent::tei:place and tei:geo]">
               <sch:assert test="@type">Dans une notice sur un lieu, si l'élément location est utilisé pour
                  spécifier des coordonnées géographiques, il devrait avoir un attribut
                  type.</sch:assert>
            </sch:rule>
         </pattern>
         <optional>
            <attribute name="when-iso">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">la forme normalisée de la période pendant laquelle le lieu décrit dans la notice a été localisé dans les subdivisions administratives indiquées</a:documentation>
               <choice>
                  <data type="date"/>
                  <data type="gYear"/>
                  <data type="gMonth"/>
                  <data type="gDay"/>
                  <data type="gYearMonth"/>
                  <data type="gMonthDay"/>
                  <data type="time"/>
                  <data type="dateTime"/>
                  <data type="token">
                     <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
                  </data>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">le type de localisation</a:documentation>
               <choice>
                  <value>geographicalCoordinates</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(coordonnées géographiques) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-occupation">
      <element name="occupation">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(activité) contient une description non formalisée de l'activité, de la profession ou de l'occupation d'une personne.si la profession du testateur est connue, cet élément permet de consigner cette profession, sous la forme d'un mot-clé nom commun simple ou composé, comme "garçon de café", "employé de bureau", "artisan", "ouvrier". [15.2.2. The Participant Description]</a:documentation>
         <text/>
      </element>
   </define>
   <define name="TEI-org">
      <element name="org">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(organisation) fournit des informations sur une organisation identifiable, telle qu'une entreprise, une tribu ou tout autre groupement de personnes.contient l'ensemble des informations connues au moment de l'export de la notice, sur une unité militaire donnée : au moins son nom en langage naturel à la date de rédaction du testament ; optionnellement : des éléments historiques rédigés, des références bibliographiques, les identifiants uniques (URI) de l'organisation dans des systèmes d'information externes. Porte obligatoirement deux attributs : xml:id (identifiant unique de l'organisation) et resp (identifiants des contributeurs). [13.2.2. Organizational Names]</a:documentation>
         <group>
            <oneOrMore>
               <ref name="TEI-orgName"/>
            </oneOrMore>
            <optional>
               <ref name="TEI-note"/>
            </optional>
            <zeroOrMore>
               <ref name="TEI-bibl"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="TEI-idno"/>
            </zeroOrMore>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-org-orgIdPattern-constraint-rule-69">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:org">
               <sch:assert test="matches(@xml:id, 'o-\d\d\d\d')">l'identifiant de l'unité militaire doit
                  commencer par le préfixe 'o-', puis un nombre sur quatre
                digits.</sch:assert>
            </sch:rule>
         </pattern>
         <attribute name="xml:id">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l'identifiant unique de l'unité militaire dans le cadre du projet.</a:documentation>
            <data type="ID"/>
         </attribute>
         <optional>
            <attribute name="resp">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <list>
                  <oneOrMore>
                     <data type="anyURI"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-person">
      <element name="person">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(personne) fournit des informations sur un individu identifiable, par exemple un participant à une interaction linguistique, ou une personne citée dans une source historique.contient l'ensemble des informations connues au moment de l'export de la notice, sur un testateur donné : au moins ses nom et prénoms, date et lieu de décès, mention de décès portée sur le testament, lien vers notice Mémoire des hommes ; optionnellement : date et lieu de naissance, lieu de résidence, profession, grade et unité d'affectation, éléments biographiques rédigés, références bibliographiques, identifiants uniques (URI) dans des systèmes d'information externes. Porte obligatoirement deux attributs : xml:id (identifiant unique de la personne) et resp (identifiants des contributeurs). [13.3.2. The Person Element 15.2.2. The Participant Description]</a:documentation>
         <group>
            <group>
               <ref name="TEI-persName"/>
               <ref name="TEI-persName"/>
            </group>
            <optional>
               <ref name="TEI-birth"/>
            </optional>
            <ref name="TEI-death"/>
            <optional>
               <ref name="TEI-occupation"/>
            </optional>
            <optional>
               <ref name="TEI-residence"/>
            </optional>
            <zeroOrMore>
               <ref name="TEI-affiliation"/>
            </zeroOrMore>
            <optional>
               <ref name="TEI-note"/>
            </optional>
            <zeroOrMore>
               <ref name="TEI-bibl"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="TEI-idno"/>
            </zeroOrMore>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-person-personIdPattern-constraint-rule-70">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:person">
               <sch:assert test="matches(@xml:id, 'p-\d\d\d\d')">l'identifiant de la personne doit commencer
                  par le préfixe 'p-', puis un nombre sur quatre
              digits.</sch:assert>
            </sch:rule>
         </pattern>
         <attribute name="xml:id">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l'identifiant unique de la personne dans le cadre du projet.</a:documentation>
            <data type="ID"/>
         </attribute>
         <optional>
            <attribute name="resp">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <list>
                  <oneOrMore>
                     <data type="anyURI"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-place">
      <element name="place">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu) contient des informations sur un lieu.contient l'ensemble des informations connues au moment de l'export de la notice, sur un lieu donné : au moins son nom à la date de rédaction du testament (et si son nom a changé depuis, son nom actuel), sa localisation dans un pays et pour la France dans un département ou une région ; optionnellement : ses coordonnées géographiques, des éléments historiques rédigés, un lien vers la base de données Geonames, des références bibliographiques, des identifiants uniques (URI) du lieu dans des systèmes d'information externes. Porte obligatoirement trois attributs : xml:id (identifiant unique du lieu), resp (identifiants des contributeurs) et type (pour spécifier à quelle catégorie de lieu on a affaire). [13.3.4. Places]</a:documentation>
         <group>
            <group>
               <ref name="TEI-placeName"/>
               <optional>
                  <ref name="TEI-placeName"/>
               </optional>
            </group>
            <oneOrMore>
               <ref name="TEI-location"/>
            </oneOrMore>
            <optional>
               <ref name="TEI-note"/>
            </optional>
            <zeroOrMore>
               <ref name="TEI-bibl"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="TEI-idno"/>
            </zeroOrMore>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-place-placeIdPattern-constraint-rule-71">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:place">
               <sch:assert test="matches(@xml:id, 'pl-\d\d\d\d')">l'identifiant du lieu doit commencer par le
                  préfixe 'pl-', puis un nombre sur quatre
              digits.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-place-attrType-constraint-rule-72">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:place">
               <sch:assert test="@type">Vous devriez
                  spécifier,à l'aide de l'attribut type, de quel type de lieu il
                  s'agit.</sch:assert>
            </sch:rule>
         </pattern>
         <attribute name="xml:id">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l'identifiant unique du lieu dans le cadre du projet.</a:documentation>
            <data type="ID"/>
         </attribute>
         <optional>
            <attribute name="resp">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <list>
                  <oneOrMore>
                     <data type="anyURI"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(le type de lieu) </a:documentation>
            <choice>
               <value>city</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(ville) </a:documentation>
               <value>locality</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(localité) pour tout lieu-dit, hameau, peuplement qui n'a pas le statut administratif de municipalité</a:documentation>
               <value>mountain</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(mont) pour les montagnes, buttes, collines</a:documentation>
               <value>forest</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(pour les forêts et bois) </a:documentation>
               <value>other</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(autre type de lieu ) </a:documentation>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="TEI-residence">
      <element name="residence">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(résidence) décrit les lieux de résidence présents ou passés d'une personne.si le lieu de résidence du testateur au moment de son décès est connu, cet élément contient, au sein d'une phrase, les informations sur ce lieu de résidence : le plus souvent, un nom de lieu (élément placeName) au moins, et le cas échéant une adresse (élément address). S'il existe une notice sur le lieu de résidence, l'élément placeName devrait logiquement avoir un attribut ref renvoyant à cette notice. [15.2.2. The Participant Description]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-hi"/>
               <ref name="TEI-placeName"/>
               <ref name="TEI-address"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-residence-usePlaceNameInResidence-constraint-rule-73">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:residence">
               <sch:assert test="tei:placeName">Si vous
                  connaissez le domicile du testateur, vous devriez utiliser l'élément placeName
                  pour baliser le nom de la ville de résidence.</sch:assert>
            </sch:rule>
         </pattern>
      </element>
   </define>
   <define name="TEI-app">
      <element name="app">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(entrée d'apparat critique) contient une entrée dans un apparat critique, constituée d'un lemme facultatif et d'au moins une leçon.L'élément app est utilisé dans le corps de la transcription, lorsqu'on observe un micro-phénomène textuel (addition, suppression, abréviation, erreur de syntaxe, dommage matériel, texte superflu, etc.) que l'on souhaite commenter par une note d'apparat critique. Il contient obligatoirement un élément lem (englobant le texte transcrit) suivi d'un élément note.L'élément app sert à englober deux éléments, dans l'ordre ci-dessous :  * l'élément lem, qui contient le mot ou l'expression du texte du testament sur lequel portera la note d'apparat critique. Exemple fictif : je done à mon aipouze tout mes biensje donne à mon épouse tous mes biens * l'élément note, qui contient la note d'apparat critique que vous souhaitez créer pour commenter une particularité d'écriture présente dans ce mot ou cette expression. Exemple fictif (pour annoter l'élément lem donné en exemple ci-dessus) : il s'agit des seules fautes d'orthographe rencontrées dans ce testament par ailleurs très bien écrit [12.1.1. The Apparatus Entry]</a:documentation>
         <group>
            <ref name="TEI-lem"/>
            <ref name="TEI-note"/>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-app-appContent-constraint-rule-74">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:app">
               <sch:assert test="tei:lem and tei:note">Si vous
                  utilisez une entrée d'apparat critique, elle doit obligatoirement comporter à la
                  fois le balisage de la partie de texte transcrite objet de la note d'apparat
                  critique, et la note d'apparat critique.</sch:assert>
            </sch:rule>
         </pattern>
         <optional>
            <attribute name="xml:id">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifiant) (identifiant unique de l'entrée d'apparat critique) fournit un identifiant unique pour l'élément qui porte l'attribut</a:documentation>
               <data type="ID"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-lem">
      <element name="lem">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lemme (texte de base)) contient le lemme ou le texte de base d'une variante textuelleDans le cadre de ce projet, comme il n'existe qu'un seul témoin textuel pour un testament donné, l'élément lem est utilisé, au sein de l'entrée d'apparat critique app, pour saisir la transcription d'une partie du texte que l'on souhaite commenter par une note d'apparat critique. [12.1. The Apparatus Entry, Readings, and Witnesses]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-lb"/>
               <ref name="TEI-pb"/>
               <ref name="TEI-choice"/>
               <ref name="TEI-gap"/>
               <ref name="TEI-unclear"/>
               <ref name="TEI-supplied"/>
               <ref name="TEI-damage"/>
               <ref name="TEI-add"/>
               <ref name="TEI-del"/>
               <ref name="TEI-subst"/>
               <ref name="TEI-hi"/>
               <ref name="TEI-placeName"/>
               <ref name="TEI-date"/>
               <ref name="TEI-persName"/>
               <ref name="TEI-orgName"/>
               <ref name="TEI-seg"/>
               <ref name="TEI-term"/>
               <ref name="TEI-foreign"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-lem-lemSibling-constraint-rule-75">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:lem">
               <sch:assert test="following-sibling::*[1][self::tei:note]">Si vous utilisez l'élément lem, il
                  doit être suivi, dans l'élément note, de la note d'apparat critique roprement
                  dite.</sch:assert>
            </sch:rule>
         </pattern>
      </element>
   </define>
   <define name="TEI-variantEncoding">
      <element name="variantEncoding">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Méthode d'encodage des variantes) sert à déclarer la méthode utilisée pour encoder les variantes critiques du texte. [12.1.1. The Apparatus Entry]</a:documentation>
         <empty/>
         <ref name="TEI-att.global.attributes"/>
         <attribute name="method">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indique quelle méthode est utilisée pour encoder des variantes dans l’apparat critique.</a:documentation>
            <choice>
               <value>location-referenced</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l’apparat critique se réfère aux numéros de ligne ou à tout autre schéma de référence canonique contenu dans le texte de base.</a:documentation>
               <value>double-end-point</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l’apparat critique indique la localisation précise du début et de la fin de chaque lemme dans le texte de base.</a:documentation>
               <value>parallel-segmentation</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">différentes leçons d’un passage sont données en parallèle dans le texte ; la notion de texte de base n’est pas nécessaire.</a:documentation>
            </choice>
         </attribute>
         <attribute name="location">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indique si l’apparat critique est intérieur ou extérieur au texte.</a:documentation>
            <choice>
               <value>internal</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l’apparat critique est intérieur au texte.</a:documentation>
               <value>external</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l’apparat critique est extérieur au texte.</a:documentation>
            </choice>
         </attribute>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-variantEncoding-location-variantEncodingLocation-constraint-rule-76">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:variantEncoding">
               <sch:assert test="(@location != 'external') or (@method != 'parallel-segmentation')">
              The @location value "external" is inconsistent with the
              parallel-segmentation method of apparatus markup.</sch:assert>
            </sch:rule>
         </pattern>
         <empty/>
      </element>
   </define>
   <define name="TEI-TEI">
      <element name="TEI">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(document TEI) contient un seul document conforme à la TEI, qui comprend un en-tête TEI et un texte, soit de façon isolée, soit comme partie d’un élément teiCorpus. [4. Default Text Structure 15.1. Varieties of Composite Text]</a:documentation>
         <group>
            <ref name="TEI-teiHeader"/>
            <oneOrMore>
               <ref name="TEI-model.resourceLike"/>
            </oneOrMore>
         </group>
         <ns xmlns="http://purl.oclc.org/dsdl/schematron"
             prefix="tei"
             uri="http://www.tei-c.org/ns/1.0"/>
         <ns xmlns="http://purl.oclc.org/dsdl/schematron"
             prefix="xs"
             uri="http://www.w3.org/2001/XMLSchema"/>
         <ns xmlns="http://purl.oclc.org/dsdl/schematron"
             prefix="rng"
             uri="http://relaxng.org/ns/structure/1.0"/>
